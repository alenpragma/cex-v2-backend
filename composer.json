{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "ext-json": "*",
        "aloha/twilio": "^4.0",
        "amphp/websocket-client": "^1.0",
        "anhskohbo/no-captcha": "^3.3",
        "azimolabs/apple-sign-in-php-sdk": "^2.2",
        "beyondcode/laravel-websockets": "^1.4",
        "doctrine/dbal": "^3.4",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "google/apiclient": "^2.15.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^3.6",
        "jenssegers/agent": "^2.6",
        "jms/serializer": "^3.24",
        "joedixon/laravel-translation": "^2.1",
        "laravel/framework": "^8.65",
        "laravel/horizon": "^5.9",
        "laravel/legacy-factories": "^1.3",
        "laravel/passport": "^10.2",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^5.7",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.2",
        "nwidart/laravel-modules": "6.0",
        "phplaravel/php-laravel": "^1.0",
        "pragmarx/google2fa-laravel": "^0.2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^4.1",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "sdtech/bitgo-laravel": "^1.0",
        "sdtech/coinpayments-laravel": "^1.0",
        "sdtech/file-uploader-laravel": "^1.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "stripe/stripe-php": "^9.6",
        "symfony/symfony": "5.4.x",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helper/helpers.php",
            "app/Helper/helpers2.php",
            "app/Helper/coreconstant.php",
            "app/Helper/corearray.php",
            "app/Helper/botHelpers.php",
            "app/Helper/bcMath.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
